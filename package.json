{
  "name": "native-base",
  "description": "Essential cross-platform UI components for React Native",
  "keywords": [
    "android",
    "ios",
    "react",
    "native",
    "nativebase",
    "react-native",
    "native-modules",
    "react-component",
    "navbar",
    "packager",
    "rnpm"
  ],
  "scripts": {
    "compile": "rm -rf dist/* && babel . --out-dir dist --ignore node_modules,dist --source-maps",
    "test": "jest",
    "prepublish": "npm run compile"
  },
  "jest": {
    "preset": "react-native",
    "transformIgnorePatterns": [
      "node_modules/(?!react-native|@shoutem|react-clone-referenced-element)"
    ]
  },
  "version": "2.0.0-beta",
  "private": false,
  "dependencies": {
    "@shoutem/animation": "^0.8.9",
    "@shoutem/theme": "git+https://github.com/GeekyAnts/theme.git",
    "blueimp-md5": "^2.5.0",
    "clamp": "^1.0.1",
    "color": "~0.11.1",
    "lodash": "~4.11.1",
    "react-native-checkbox": "~1.0.8",
    "react-native-easy-grid": "0.1.8",
    "react-native-keyboard-aware-scroll-view": "0.2.0",
    "react-native-scrollable-tab-view": "^0.7.1",
    "react-native-vector-icons": "~4.0.0",
    "react-tween-state": "^0.1.5",
    "tween-functions": "^1.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-eslint": "^6.1.2",
    "babel-jest": "^16.0.0",
    "babel-preset-react-native": "^1.9.0",
    "eslint": "^3.5.0",
    "eslint-config-airbnb": "^11.1.0",
    "eslint-plugin-import": "^1.14.0",
    "eslint-plugin-jsx-a11y": "^2.2.1",
    "eslint-plugin-react": "^6.2.0",
    "eslint-plugin-react-native": "^2.0.0",
    "jest": "17.0.0",
    "react": "^15.4.2",
    "react-native": "^0.39.2",
    "react-test-renderer": "^15.3.2"
  },
  "readmeFilename": "README.md",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GeekyAnts/NativeBase.git"
  },
  "rnpm": {
    "assets": [
      "Fonts"
    ]
  }
}
